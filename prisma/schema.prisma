  uid    Int
  user   user    @relation(fields: [uid], references: [uid], onDelete: Cascade, onUpdate: NoAction, map: "zixuan_uid")

  @@id([fid, uid])
  @@index([uid], map: "zixuan_user_uid_fk")
}

model funds {
  fid    String @id @db.Char(6)
  pinyin String @db.VarChar(255)
  name   String @db.VarChar(255)
  leixin String @db.VarChar(255)
  rate   Float?
  value  Float?
}
